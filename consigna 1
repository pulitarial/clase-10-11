//consigna 1//
#include <stdio.h>
#include <string.h>

int b [10];
int i;

int main()

{   
    for (int i=0;i<10;i++)
        {printf("introdusca los 10 valores del array b\n");
        scanf("%d",&b[i]); }
    
    printf("listado de valores:\n");
    for (int i=0;i<10;i++)
        {printf("- %d\n", b[i]);
        if ((i==0)&&(b[0]<0))
            printf("negativo\n");
          if ((i==4)&& (b[4]>0))
            printf("positivo\n");
            if ((i==9)&&(b[9]!=0))
                printf("el decimo valor es distinto de cero\n");
        
        }
    return 0;
}
